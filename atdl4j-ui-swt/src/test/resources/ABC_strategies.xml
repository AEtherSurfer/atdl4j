						</flow:stateRule>
					</parameter>
					<parameter name="MaxDisplayQty" type="7" xsi:type="Qty_t" uiRep="Max Display Qty" fixTag="8088" use="optional" lay:controlType="TextField">
						<flow:stateRule enabled="true">
